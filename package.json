{
  "name": "node-sequelize-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Phirathat Jittieng",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && babel src --out-dir dist",
    "clean": "rimraf dist",
    "dev": "nodemon --exec yarn restart",
    "restart": "yarn build && yarn start",
    "start": "node dist/index.js",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "mariadb": "^2.4.2",
    "morgan": "^1.10.0",
    "sequelize": "^5.21.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.1",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.2.0"
  }
}
